{
    "name": "find-it",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "jest",
        "prettier:check": "prettier --config .prettierrc --check \"src/**/*.{ts,css,html}\"",
        "prepare": "husky install",
        "commit": "git-cz",
        "dev:ssr": "ng run findIt:serve-ssr",
        "serve:ssr": "node dist/findIt/server/main.js",
        "build:ssr": "ng build && ng run findIt:server",
        "prerender": "ng run findIt:prerender"
    },
    "lint-staged": {
        "src/**/*.{ts,css,html}": [
            "npm run prettier:check"
        ]
    },
    "jest": {
        "preset": "jest-preset-angular",
        "setupFilesAfterEnv": [
            "<rootDir>/setupJest.ts"
        ]
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^14.1.3",
        "@angular/common": "^14.1.3",
        "@angular/compiler": "^14.1.3",
        "@angular/core": "^14.1.3",
        "@angular/forms": "^14.1.3",
        "@angular/platform-browser": "^14.1.3",
        "@angular/platform-browser-dynamic": "^14.1.3",
        "@angular/platform-server": "^14.1.3",
        "@angular/router": "^14.1.3",
        "@angular/service-worker": "^14.1.3",
        "@nguniversal/express-engine": "^14.1.0",
        "express": "^4.15.2",
        "normalize.css": "^8.0.1",
        "primeicons": "^5.0.0",
        "primeng": "^14.0.1",
        "rxjs": "~7.5.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^14.0.0",
        "@angular/cli": "~14.0.0",
        "@angular/compiler-cli": "^14.1.3",
        "@nguniversal/builders": "^14.1.0",
        "@types/express": "^4.17.0",
        "@types/jasmine": "~4.0.0",
        "@types/node": "^14.15.0",
        "husky": "^8.0.1",
        "jasmine-core": "~4.1.0",
        "jest": "^28.1.3",
        "jest-preset-angular": "^12.2.0",
        "karma-coverage": "~2.2.0",
        "lint-staged": "^13.0.3",
        "prettier": "^2.7.1",
        "typescript": "~4.7.2"
    }
}
